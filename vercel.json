{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/child",
      "dest": "/kid_ui.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
